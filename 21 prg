#include <stdio.h>
int main() {
    int n,largest;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    int array[n];
    for(int i=0;i<n;i++)
        scanf("%d",&array[i]);
    largest=array[0];
    for(int i=1;i<n;i++)
        if(array[i]>largest)largest=array[i];
    printf("%d\n",largest);
    return 0;
}
